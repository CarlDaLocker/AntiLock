	Callback = function()
      		loadstring(game:HttpGet("https://raw.githubusercontent.com/CarlDaLocker/AntiLock/main/DUKE%20SHADOW%20SILENT"))()
  	end    
})

--[[
Name = <string> - The name of the button.
Callback = <function> - The function of the button.
]]

